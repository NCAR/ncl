MYNAME  = nnalg

SRCS = nadisp.f  nadraw.f  naskln.f  nasort.f
OBJS = nadisp.o  nadraw.o  naskln.o  nasort.o

#if defined(Darwin)
PROG_LIBS	=	$(NCARGLIBS) $(CTOFLIBS) $(PNGLIB)
#else
PROG_LIBS	=	$(NCARGLIBS) $(PNGLIB)
#endif

DEP_LIBS = $(DEPNCARGLIBS)
SYS_LIBS = $(XLIB)

FortranProgram($(MYNAME),$(OBJS),$(DEP_LIBS))

